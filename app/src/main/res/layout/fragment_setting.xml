<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".ui.SettingFragment">
    <TextView
        android:id="@+id/tv_settings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="40dp"
        android:layout_marginStart="14dp"
        android:text="@string/settings"
        android:textSize="26sp"
        android:fontFamily="@font/robotobold"
        android:textColor="@color/black"
        />
    <View
        android:id="@+id/setting_view1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_settings"
        android:layout_marginTop="16dp"
        android:background="#8D98A1"
        />
    <ImageView
        android:id="@+id/iv_change_lan"
        android:layout_width="32dp"
        android:layout_height="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/setting_view1"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:src="@mipmap/ic_change_language"
        />
    <TextView
        android:id="@+id/tv_change_lan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/iv_change_lan"
        app:layout_constraintTop_toTopOf="@id/iv_change_lan"
        app:layout_constraintBottom_toBottomOf="@id/iv_change_lan"
        android:layout_marginStart="14dp"
        android:text="@string/change_language"
        android:textSize="14sp"
        android:fontFamily="@font/robotomedium"
        android:textColor="@color/black"
        />
    <ImageView
        android:id="@+id/iv_arrow_language"
        android:layout_width="24dp"
        android:layout_height="24dp"
       app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/setting_view1"
        android:layout_marginEnd="12dp"
        android:layout_marginTop="17dp"
        android:src="@mipmap/arrow"
        />
    <View
        android:id="@+id/setting_view2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv_change_lan"
        android:layout_marginTop="16dp"
        android:background="#8D98A1"
        />
    <ImageView
        android:id="@+id/iv_about_us"
        android:layout_width="32dp"
        android:layout_height="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/setting_view2"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:src="@mipmap/ic_change_language"
        />
    <TextView
        android:id="@+id/tv_about_us"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/iv_about_us"
        app:layout_constraintTop_toTopOf="@id/iv_about_us"
        app:layout_constraintBottom_toBottomOf="@id/iv_about_us"
        android:layout_marginStart="14dp"
        android:text="@string/about_us"
        android:textSize="14sp"
        android:fontFamily="@font/robotomedium"
        android:textColor="@color/black"
        />
    <ImageView
        android:id="@+id/iv_arrow_about_us"
        android:layout_width="24dp"
        android:layout_height="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/setting_view2"
        android:layout_marginEnd="12dp"
        android:layout_marginTop="17dp"
        android:src="@mipmap/arrow"
        />
    <View
        android:id="@+id/setting_view3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv_about_us"
        android:layout_marginTop="16dp"
        android:background="#8D98A1"
        />
    <ImageView
        android:id="@+id/iv_faq"
        android:layout_width="32dp"
        android:layout_height="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/setting_view3"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:src="@mipmap/ic_change_language"
        />
    <TextView
        android:id="@+id/tv_faq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/iv_faq"
        app:layout_constraintTop_toTopOf="@id/iv_faq"
        app:layout_constraintBottom_toBottomOf="@id/iv_faq"
        android:layout_marginStart="14dp"
        android:text="@string/faq_s"
        android:textSize="14sp"
        android:fontFamily="@font/robotomedium"
        android:textColor="@color/black"
        />
    <ImageView
        android:id="@+id/iv_arrow_faq"
        android:layout_width="24dp"
        android:layout_height="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/setting_view3"
        android:layout_marginEnd="12dp"
        android:layout_marginTop="17dp"
        android:src="@mipmap/arrow"
        />
    <View
        android:id="@+id/setting_view4"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv_faq"
        android:layout_marginTop="16dp"
        android:background="#8D98A1"
        />
    <ImageView
        android:id="@+id/iv_terms"
        android:layout_width="32dp"
        android:layout_height="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/setting_view4"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:src="@mipmap/ic_change_language"
        />
    <TextView
        android:id="@+id/tv_terms"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/iv_terms"
        app:layout_constraintTop_toTopOf="@id/iv_terms"
        app:layout_constraintBottom_toBottomOf="@id/iv_terms"
        android:layout_marginStart="14dp"
        android:text="Terms And Conditions"
        android:textSize="14sp"
        android:fontFamily="@font/robotomedium"
        android:textColor="@color/black"
        />
    <ImageView
        android:id="@+id/iv_arrow_terms"
        android:layout_width="24dp"
        android:layout_height="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/setting_view4"
        android:layout_marginEnd="12dp"
        android:layout_marginTop="17dp"
        android:src="@mipmap/arrow"
        />
    <View
        android:id="@+id/setting_view5"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv_terms"
        android:layout_marginTop="16dp"
        android:background="#8D98A1"
        />
    <ImageView
        android:id="@+id/iv_logout"
        android:layout_width="32dp"
        android:layout_height="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/setting_view5"
        android:layout_marginStart="20dp"
        android:layout_marginTop="15dp"
        android:src="@mipmap/ic_change_language"
        />
    <TextView
        android:id="@+id/tv_logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/iv_logout"
        app:layout_constraintTop_toTopOf="@id/iv_logout"
        app:layout_constraintBottom_toBottomOf="@id/iv_logout"
        android:layout_marginStart="14dp"
        android:text="@string/logout"
        android:textSize="14sp"
        android:fontFamily="@font/robotomedium"
        android:textColor="@color/black"
        />
    <ImageView
        android:id="@+id/iv_arrow_logout"
        android:layout_width="24dp"
        android:layout_height="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/setting_view5"
        android:layout_marginEnd="12dp"
        android:layout_marginTop="17dp"
        android:src="@mipmap/arrow"
        />
    <View
        android:id="@+id/setting_view6"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv_logout"
        android:layout_marginTop="16dp"
        android:background="#8D98A1"
        />


</androidx.constraintlayout.widget.ConstraintLayout>